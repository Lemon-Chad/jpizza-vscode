{
    "name": "jpizza-lang",
    "publisher": "Jackson Smith",
    "displayName": "JPizza Language Support",
    "description": "Adds support for the JPizza2 programming language!",
    "version": "0.0.1",
    "icon": "dp256.png",
    "repository": {
        "type": "git",
        "url": "https://github.com/Lemon-Chad/jpizza-vscode.git"
    },
    "engines": {
        "vscode": "^1.49.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "jpizza",
            "aliases": ["JPizza2", "jpizza", "JPizza", "jpizza2"],
            "extensions": [
                ".devp",
                ".jbox"
            ],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "jpizza",
            "scopeName": "src.devp",
            "path": "./syntaxes/jpizza.tmLanguage.json"
        }]
    }
}